{
  "name": "Polymer-with-Angular",
  "author":"Vinod Varghese",
  "version": "1.0.0",
  "scripts": {
    "dev-tsc": "./node_modules/.bin/tsc",
    "dev-tsc:w": "./node_modules/.bin/tsc -w",
